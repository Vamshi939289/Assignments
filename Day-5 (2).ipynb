        "from collections import Counter\n",
        "\n",
        "def word_frequency(text):\n",
        "    words = text.lower().split()\n",
        "    word_counts = Counter(words)\n",
        "\n",
        "    for word, count in word_counts.items():\n",
        "        print(f\"{word}: {count}\")\n",
        "\n",
        "text = input()\n",
        "word_frequency(text)\n"
      ]
